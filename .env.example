# TutorConnect Environment Variables 
 
# Database Configuration (Backend Services) 
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
 
# Frontend (Vite requires VITE_ prefix) 
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# JWT Configuration 
JWT_SECRET=your_jwt_secret_key
 
# Frontend Configuration 
FRONTEND_URL=http://localhost:3000 
 
# Service Ports 
AUTH_SERVICE_PORT=3001 
USERS_SERVICE_PORT=3002 
PROFILES_SERVICE_PORT=3003 
BOOKINGS_SERVICE_PORT=3004 
MESSAGING_SERVICE_PORT=3005 
REVIEWS_SERVICE_PORT=3006 
NOTIFICATIONS_SERVICE_PORT=3007 
ANALYTICS_SERVICE_PORT=3008 
GAMIFICATION_SERVICE_PORT=3009 
EARNINGS_SERVICE_PORT=3010 
CALENDAR_SERVICE_PORT=3011 
MAPS_SERVICE_PORT=3012 
 
# Message Queue (RabbitMQ) 
RABBITMQ_URL=amqp://localhost:5672 
 
# Development
NODE_ENV=development

# Twilio Configuration (for SMS notifications)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
